    "def lone_sum(a, b, c):\n",
    "  sum = 0\n",
    "  sum += a if a not in [b,c] else 0\n",
    "  sum += b if b not in [a,c] else 0\n",
    "  sum += c if c not in [a,b] else 0\n",
    "  return sum\n"
